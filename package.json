{
  "name": "styling-solutions-rfc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:all": "nx run-many --target=build",
    "lint:all": "nx run-many --target=lint",
    "check:types:all": "nx run-many --target=check:types",
    "graph": "nx graph",
    "linaria": "pnpm --filter linaria",
    "griffel": "pnpm --filter griffel",
    "compiled": "pnpm --filter compiled",
    "styled-components": "pnpm --filter styled-components",
    "tailwind": "pnpm --filter tailwind",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "nx": "15.2.1",
    "prettier": "^2.8.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "typescript": "4.9.3"
  }
}
